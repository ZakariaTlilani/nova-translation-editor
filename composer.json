{
    "name": "zakariatlilani/nova-translation-editor",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "laravel/nova": "^4.0",    
        "spatie/laravel-translation-loader": ">=2.6.3"

    },
    "autoload": {
        "psr-4": {
            "Zakariatlilani\\NovaTranslationEditor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zakariatlilani\\NovaTranslationEditor\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
